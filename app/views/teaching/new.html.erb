<%= debug current_person %>

<% form_for :course do |f| %>
  <div>
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>
  <div>
    <%= f.label :description %>
    <%= f.text_field :description %>
  </div>
  <div>
    <%= f.label :start, 'from' %>
    <%= f.text_field :start %>
    <%= f.label :end, 'to' %>
    <%= f.text_field :end %>
  </div>
  <div>
    <%= f.submit %>
  </div>
<% end %>